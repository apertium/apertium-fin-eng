  </rule>

  <!-- subject position gender neutral -->
  <rule>
    <match lemma="hän" tags="prn.*">
      <select lemma="he or she" tags="prn.*"/>
    </match>
    <match tags="vblex.*"/>
  </rule>

  <!-- gender neutral they -->
  <rule>
    <match lemma="hän" tags="prn.*">
