  </rule>
  <rule>
    <match lemma="ääni" tags="n.*.ins"><select lemma="vote" tags="n.*"/></match>
  </rule>
  <rule>
    <match lemma="ääni" tags="n.*"><select lemma="sound" tags="n.*"/></match>
  </rule>
