</rule>

  <!-- pronominal gender selection mess -->

  <!-- feminine stuffs to the left -->
  <rule>
    <match tags="np.ant.f.*"/>
    <match/>
    <match lemma="hän" tags="prn.*">
      <select lemma="he" tags="prn.*"/>
    </match>
  </rule>
  <rule>
    <match tags="np.ant.f.*"/>
    <match lemma="hän" tags="prn.*">
      <select lemma="he" tags="prn.*"/>
    </match>
  </rule>

  <!-- masculine stuffs to the left -->
  <rule>
    <match tags="np.ant.m.*"/>
    <match/>
    <match lemma="hän" tags="prn.*">
      <select lemma="he" tags="prn.*"/>
    </match>
  </rule>
  <rule>
    <match tags="np.ant.m.*"/>
    <match lemma="hän" tags="prn.*">
      <select lemma="he" tags="prn.*"/>
    </match>
  </rule>

  <!-- some keywords to the left -->
  <rule>
    <match lemma="poika" tags="n.*"/>
