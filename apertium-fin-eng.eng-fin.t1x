  <!-- }}}
       -->
  <section-def-vars>
    <def-var n="number"/>
    <def-var n="current_verb"/>
    <def-var n="prep-lema"/>
    <def-var n="pers"/>
    <def-var n="persnum"/>
    <def-var n="comparison"/>
    <def-var n="tense"/>
    <def-var n="possession"/>
    <def-var n="conneg"/>
    <def-var n="determiner"/>
  </section-def-vars>
  <!-- {{{ macros -->
  <section-def-macros>
    <def-macro n="compare" npar="1">
      <let><var n="comparison"/><lit-tag v="pos"/></let>
      <choose>
        <when>
          <test><equal><clip pos="1" side="sl" part="a_comp"/><lit-tag v="comp"/></equal></test>
          <let><clip pos="1" side="tl" part="a_comp"/><lit-tag v="comp"/></let>
          <let><var n="comparison"/><lit-tag v="com"/></let>
        </when>
        <when>
          <test><equal><clip pos="1" side="sl" part="a_comp"/><lit-tag v="sup"/></equal></test>
          <let><clip pos="1" side="tl" part="a_comp"/><lit-tag v="sup"/></let>
          <let><var n="comparison"/><lit-tag v="sup"/></let>
        </when>
        <otherwise>
          <let><var n="comparison"/><lit-tag v="pos"/></let>
        </otherwise>
      </choose>
    </def-macro>
    <def-macro n="possmangler1" npar="1">
      <let><var n="possession"/><lit-tag v="pxsp3"/></let>
      <choose>
        <when>
          <test><and>
              <equal><clip pos="1" side="sl" part="a_num"/><lit-tag v="sg"/></equal>
              <equal><clip pos="1" side="sl" part="a_pers"/><lit-tag v="p1"/></equal>
          </and></test>
          <let><var n="possession"/><lit-tag v="pxsg1"/></let>
        </when>
        <when>
          <test><and>
              <equal><clip pos="1" side="sl" part="a_num"/><lit-tag v="sg"/></equal>
              <equal><clip pos="1" side="sl" part="a_pers"/><lit-tag v="p2"/></equal>
          </and></test>
          <let><var n="possession"/><lit-tag v="pxsg2"/></let>
        </when>
        <when>
          <test>
            <equal><clip pos="1" side="sl" part="a_pers"/><lit-tag v="p3"/></equal>
          </test>
          <let><var n="possession"/><lit-tag v="pxsp3"/></let>
        </when>
        <when>
          <test><and>
              <equal><clip pos="1" side="sl" part="a_num"/><lit-tag v="pl"/></equal>
              <equal><clip pos="1" side="sl" part="a_pers"/><lit-tag v="p2"/></equal>
          </and></test>
          <let><var n="possession"/><lit-tag v="pxpl2"/></let>
        </when>
        <when>
          <test><and>
              <equal><clip pos="1" side="sl" part="a_num"/><lit-tag v="pl"/></equal>
              <equal><clip pos="1" side="sl" part="a_pers"/><lit-tag v="p1"/></equal>
          </and></test>
          <let><var n="possession"/><lit-tag v="pxpl1"/></let>
        </when>
        <when>
          <test>
              <equal><clip pos="1" side="sl" part="lem"/><lit v="my"/></equal>
          </test>
          <let><var n="possession"/><lit-tag v="pxsg1"/></let>
        </when>
        <when>
          <test>
              <equal><clip pos="1" side="sl" part="lem"/><lit v="your"/></equal>
          </test>
          <let><var n="possession"/><lit-tag v="pxsg2"/></let>
        </when>
        <when>
          <test>
              <equal><clip pos="1" side="sl" part="lem"/><lit v="his"/></equal>
          </test>
          <let><var n="possession"/><lit-tag v="pxsp3"/></let>
        </when>
        <when>
          <test>
              <equal><clip pos="1" side="sl" part="lem"/><lit v="her"/></equal>
          </test>
          <let><var n="possession"/><lit-tag v="pxsp3"/></let>
        </when>
        <when>
          <test>
              <equal><clip pos="1" side="sl" part="lem"/><lit v="our"/></equal>
          </test>
          <let><var n="possession"/><lit-tag v="pxpl1"/></let>
        </when>
        <when>
          <test>
              <equal><clip pos="1" side="sl" part="lem"/><lit v="their"/></equal>
          </test>
          <let><var n="possession"/><lit-tag v="pxsp3"/></let>
        </when>
        <otherwise>
          <let><var n="possession"/><lit-tag v="pxsp3"/></let>
        </otherwise>
      </choose>
    </def-macro>

      <def-macro n="test" npar="1">
          <let>
            <var n="number"/>
            <lit v=""/>
          </let>
    </def-macro>
    <def-macro n="tensemangle1" npar="1">
      <let><var n="conneg"/><lit-tag v="conneg"/></let>
        <choose>
            <when>
                <test><equal><clip pos="1" side="sl" part="a_tens"/><lit-tag v="pres"/></equal></test>
                <let><var n="tense"/><lit-tag v="pri"/></let>
            </when>
            <when>
                <test><equal><clip pos="1" side="sl" part="a_tens"/><lit-tag v="cni"/></equal></test>
                <let><var n="tense"/><lit-tag v="pri"/></let>
              </when>
            <when>
                <test><equal><clip pos="1" side="sl" part="a_tens"/><lit-tag v="past"/></equal></test>
                <let><var n="tense"/><lit-tag v="past"/></let>
                <let><var n="conneg"/><concat>
                    <lit-tag v="sg.conneg"/>
                </concat></let>
              </when>
              <otherwise>
                <let><var n="tense"/><lit-tag v="pri"/></let>
              </otherwise>
        </choose>
      </def-macro>
      <def-macro n="personmangle1" npar="1">
        <choose>
          <when>
            <test>
              <not><equal><clip pos="1" side="sl" part="a_pers"/><lit v=""/></equal></not>
            </test>
            <let><var n="pers"/><clip pos="1" side="sl" part="a_pers"/></let>
          </when>
          <otherwise>
            <let><var n="pers"/><lit-tag v="p3"/></let>
          </otherwise>
        </choose>
        <choose>
          <when>
            <test>
              <not><equal><clip pos="1" side="sl" part="a_num"/><lit v=""/></equal></not>
            </test>
            <let><var n="persnum"/><clip pos="1" side="sl" part="a_num"/></let>
          </when>
          <otherwise>
            <let><var n="persnum"/><lit-tag v="sg"/></let>
          </otherwise>
        </choose>
      </def-macro>
      <def-macro n="artmangle1" npar="1">
        <choose>
          <when>
            <test><or>
              <equal><clip pos="1" side="sl" part="lem"/><lit v="the"/></equal>
              <equal><clip pos="1" side="sl" part="lem"/><lit v="a"/></equal>
              <equal><clip pos="1" side="sl" part="lem"/><lit v="The"/></equal>
              <equal><clip pos="1" side="sl" part="lem"/><lit v="A"/></equal>
            </or></test>
            <let><var n="determiner"/><lit v=""/></let>
          </when>
          <otherwise>
            <let><var n="determiner"/>
              <concat><clip pos="1" side="tl" part="whole"/>
              <lit-tag v="2"/></concat>
            </let>
          </otherwise>
        </choose>
      </def-macro>
      <def-macro n="numbermangle1" npar="1">
        <choose>
        <when>
          <test>
            <not><equal><clip pos="1" side="sl" part="a_num"/><lit v=""/></equal></not>
          </test>
          <let><var n="number"/><clip pos="1" side="sl" part="a_num"/></let>
        </when>
        <otherwise>
          <let><var n="number"/><lit-tag v="sg"/></let>
        </otherwise>
        </choose>
      </def-macro>
  </section-def-macros>
  <section-rules>

    <!-- }}}
         RULES
         {{{ larger rules -->



    <rule comment="on day month year -> day+ess month+par year ">
      <pattern>
          <pattern-item n="on"/>
          <pattern-item n="day"/>
          <pattern-item n="month"/>
          <pattern-item n="year"/>
      </pattern>
      <action>
        <out>
          <chunk name="date" case="caseFirstWord">
            <tags>
              <tag><lit-tag v="AdvP"/></tag>
            </tags>
            <lu>
              <clip pos="2" side="tl" part="lem"/> <!-- do -->
              <clip pos="2" side="tl" part="a_numtype"/> <!-- vblex.actv -->
              <lit-tag v="sg.ess"/>
            </lu>
            <b/>
            <lu>
              <clip pos="3" side="tl" part="lem"/> <!-- do -->
              <clip pos="3" side="tl" part="nominaltype"/> <!-- vblex.actv -->
              <lit-tag v="sg.par"/>
            </lu>
            <b/>
            <lu>
              <clip pos="4" side="tl" part="lem"/> <!-- do -->
              <clip pos="4" side="tl" part="a_numtype"/> <!-- vblex.actv -->
              <lit-tag v="sg.nom"/>
            </lu>
          </chunk>
        </out>
      </action>
    </rule>
    <rule comment="will be pcp -> ">
      <pattern>
          <pattern-item n="will"/>
          <pattern-item n="be"/>
          <pattern-item n="fverb"/> <!-- play / eat / help ... -->
      </pattern>
      <action>
        <call-macro n="tensemangle1">
          <with-param pos="2"/> <!-- tense in auxv -->
        </call-macro>
        <call-macro n="personmangle1">
          <with-param pos="2"/>
        </call-macro>
        <out>
          <chunk name="futverb" case="caseFirstWord">
            <tags>
              <tag><lit-tag v="VP"/></tag>
            </tags>
            <lu>
              <clip pos="3" side="tl" part="lem"/> <!-- do -->
              <clip pos="3" side="tl" part="a_verb"/> <!-- vblex.actv -->
              <lit-tag v="pasv.pot.impers"/>
            </lu>
          </chunk>
        </out>
      </action>
    </rule>
    <rule comment="PRN-PRS-3 MOD NOT V -> prnpres negv mod infa">
      <pattern>
          <pattern-item n="prnprs-3"/> <!-- I, you, he, she, it, we, they -->
          <pattern-item n="modverb"/> <!-- can / ... -->
          <pattern-item n="notadv"/> <!-- not / n't -->
          <pattern-item n="infverb"/> <!-- play / eat / help ... -->
      </pattern>
      <action>
        <call-macro n="tensemangle1">
          <with-param pos="2"/> <!-- tense in auxv -->
        </call-macro>
        <call-macro n="personmangle1">
          <with-param pos="1"/>
        </call-macro>
        <out>
          <chunk name="subjverb" case="caseFirstWord">
            <tags>
              <tag><lit-tag v="VP.NEG"/></tag>
              <tag><var n="pers"/></tag>
              <tag><var n="persnum"/></tag>
            </tags>
            <lu>
              <clip pos="1" part="whole" side="tl"/>
            </lu>
            <b pos="1"/>
            <lu>
              <lit v="ei"/> <!-- pos=3 ~ not -->
              <lit-tag v="vblex.neg.actv"/>
              <var n="pers"/>
              <var n="persnum"/>
            </lu>
              <b pos="2"/>
            <lu>
              <clip pos="2" side="tl" part="lem"/> <!-- do -->
              <clip pos="2" side="tl" part="a_verb"/> <!-- vblex.actv -->
              <lit-tag v="actv"/>
              <var n="tense"/> <!-- pri -->
              <var n="conneg"/>
            </lu>
              <b pos="3"/>
            <lu>
              <clip pos="4" side="tl" part="lem"/>
              <clip pos="4" side="tl" part="a_verb"/> <!-- vblex.actv -->
              <lit-tag v="actv.infa.sg.lat"/>
            </lu>
          </chunk>
        </out>
      </action>
    </rule>
    <rule comment="PRN-PRS-12 MOD NOT V -> negv mod infa">
      <pattern>
          <pattern-item n="prnprs-non3"/> <!-- I, you, he, she, it, we, they -->
          <pattern-item n="modverb"/> <!-- can / ... -->
          <pattern-item n="notadv"/> <!-- not / n't -->
          <pattern-item n="infverb"/> <!-- play / eat / help ... -->
      </pattern>
      <action>
        <call-macro n="tensemangle1">
          <with-param pos="2"/> <!-- tense in auxv -->
        </call-macro>
        <call-macro n="personmangle1">
          <with-param pos="1"/>
        </call-macro>
        <out>
          <chunk name="subjverb" case="caseFirstWord">
            <tags>
              <tag><lit-tag v="VP.NEG"/></tag>
              <tag><var n="pers"/></tag>
              <tag><var n="persnum"/></tag>
            </tags>
            <lu>
              <lit v="ei"/> <!-- pos=3 ~ not -->
              <lit-tag v="vblex.neg.actv"/>
              <var n="pers"/>
              <var n="persnum"/>
            </lu>
              <b pos="2"/>
            <lu>
              <clip pos="2" side="tl" part="lem"/> <!-- do -->
              <clip pos="2" side="tl" part="a_verb"/> <!-- vblex.actv -->
              <lit-tag v="actv"/>
              <var n="tense"/> <!-- pri -->
              <var n="conneg"/>
            </lu>
              <b pos="3"/>
            <lu>
              <clip pos="4" side="tl" part="lem"/>
              <clip pos="4" side="tl" part="a_verb"/> <!-- vblex.actv -->
              <lit-tag v="actv.infa.sg.lat"/>
            </lu>
          </chunk>
        </out>
      </action>
    </rule>

    <rule comment="3SG-stuff VAUX NOT V -> stuff ei voi">
      <pattern>
          <pattern-item n="sg3stuff"/> <!-- dog / Mary / ...  -->
