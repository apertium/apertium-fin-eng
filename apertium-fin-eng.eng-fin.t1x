              <clip pos="1" side="tl" part="lem"/> <!-- tehdä -->
              <clip pos="1" side="tl" part="a_verb"/> <!-- vblex.actv -->
              <lit-tag v="pasv.pp.pos.sg.nom"/> <!-- infma -->
            </lu>
          </chunk>
        </out>
      </action>
    </rule>
    <rule comment="vbsubs -> infminen">
      <pattern>
          <pattern-item n="subsverb"/> <!-- do -->
      </pattern>
      <action>
        <out>
          <chunk name="vbsubs" case="caseFirstWord">
            <tags>
              <tag><lit-tag v="V-SUBS"/></tag>
            </tags>
            <lu>
              <clip pos="1" side="tl" part="lem"/> <!-- tehdä -->
              <clip pos="1" side="tl" part="a_verb"/> <!-- vblex.actv -->
              <lit-tag v="infminen"/> <!-- infminen -->
            </lu>
          </chunk>
        </out>
      </action>
    </rule>
    <rule comment="gerund -> infma ssa">
      <pattern>
          <pattern-item n="ger"/> <!-- do -->
      </pattern>
      <action>
        <out>
          <chunk name="ger" case="caseFirstWord">
            <tags>
              <tag><lit-tag v="V-INF"/></tag>
            </tags>
            <lu>
              <clip pos="1" side="tl" part="lem"/> <!-- tehdä -->
              <clip pos="1" side="tl" part="a_verb"/> <!-- vblex.actv -->
              <lit-tag v="actv.infma.sg.ine"/> <!-- infma -->
               <!-- ine or ill FIXME -->
            </lu>
          </chunk>
        </out>
      </action>
    </rule>
    <rule comment="num: add sg noms">
      <pattern>
          <pattern-item n="num"/> <!-- 1, one-->
      </pattern>
      <action>
        <out>
          <chunk name="num" case="caseFirstWord">
            <tags>
              <tag><lit-tag v="NumP"/></tag>
              <tag><lit-tag v="CD"/></tag>
            </tags>
            <lu>
              <clip pos="1" side="tl" part="lem"/> <!-- 1 -->
              <clip pos="1" side="tl" part="a_numtype"/> <!-- num.card -->
              <lit-tag v="sg.nom"/> <!-- so far (was: 2) -->
            </lu>
          </chunk>
        </out>
      </action>
    </rule>

    <rule comment="det n's -> n+gen">
      <pattern>
          <pattern-item n="det"/> <!-- the -->
          <pattern-item n="noun"/> <!-- boy -->
          <pattern-item n="gen"/> <!-- 's -->
      </pattern>
      <action>
        <call-macro n="artmangle1">
            <with-param pos="1"/>
        </call-macro>
        <out>
          <chunk name="np" case="caseFirstWord">
            <tags>
              <tag><lit-tag v="NP.GEN"/></tag>
            </tags>
            <lu>
              <var n="determiner"/>
            </lu>
            <b pos="1"/>
            <lu>
              <clip pos="2" side="tl" part="lem"/> <!-- boy -->
              <clip pos="2" side="tl" part="nominaltype"/> <!-- n -->
              <clip pos="2" side="tl" part="a_num"/> <!-- sg, pl -->
              <lit-tag v="gen"/>
            </lu>
          </chunk>
        </out>
      </action>
    </rule>
    <rule comment="n's -> n+gen">
      <pattern>
          <pattern-item n="noun"/> <!-- boy -->
          <pattern-item n="gen"/> <!-- 's -->
      </pattern>
      <action>
        <out>
          <chunk name="np" case="caseFirstWord">
            <tags>
              <tag><lit-tag v="NP.GEN"/></tag>
            </tags>
            <lu>
              <clip pos="1" side="tl" part="lem"/> <!-- boy -->
              <clip pos="1" side="tl" part="nominaltype"/> <!-- n -->
              <clip pos="1" side="tl" part="a_num"/> <!-- sg, pl -->
              <lit-tag v="gen"/>
            </lu>
          </chunk>
        </out>
      </action>
    </rule>

    <rule comment="SäÄntö: n">
      <pattern>
          <pattern-item n="noun"/> <!-- boy -->
      </pattern>
      <action>
        <out>
          <chunk name="np" case="caseFirstWord">
            <tags>
              <tag><lit-tag v="NP"/></tag>
              <tag><lit-tag v="CD"/></tag>
            </tags>
            <lu>
              <clip pos="1" side="tl" part="lem"/> <!-- boy -->
              <clip pos="1" side="tl" part="nominaltype"/> <!-- n -->
              <clip pos="1" side="tl" part="a_num"/> <!-- sg, pl -->
              <lit-tag v="2"/>
            </lu>
          </chunk>
        </out>
      </action>
    </rule>

    <!-- sometimes adjectives are stranded, or predicative, but where to
         determine the case and number from -->
    <rule comment="SäÄntö: a is so lonely">
      <pattern>
          <pattern-item n="adj"/> <!-- little -->
      </pattern>
      <action>
        <call-macro n="comparison">
            <with-param pos="1"/>
        </call-macro>
        <out>
            <chunk name="ap" case="caseFirstWord">
                <tags>
                    <tag><lit-tag v="AP"/></tag>
                    <tag><lit-tag v="CD"/></tag>
                </tags>
            <lu>
              <!-- no det in fin -->
              <clip pos="1" side="tl" part="lem"/> <!-- little -->
              <clip pos="1" side="tl" part="a_adj"/> <!-- adj -->
              <var n="comparison"/>
              <lit-tag v="sg.nom"/>
            </lu>
          </chunk>
        </out>
      </action>
    </rule>

    <rule comment="Säätö: prn">
      <pattern>
        <pattern-item n="prn"/>
      </pattern>
      <action>
        <call-macro n="numbermangle1">
          <with-param pos="1"/>
        </call-macro>
        <out>
          <chunk name="np" case="caseFirstWord">
            <tags>
              <tag><lit-tag v="NP"/></tag>
              <tag><lit-tag v="CD"/></tag>
            </tags>
            <lu>
              <clip pos="1" side="tl" part="lem"/> <!-- boy -->
              <clip pos="1" side="tl" part="nominaltype"/> <!-- n -->
              <var n="number"/> <!-- sg, pl -->
              <lit-tag v="2"/>
            </lu>
          </chunk>
        </out>
      </action>
    </rule>

    <rule comment="Säätö: pr">
      <pattern>
        <pattern-item n="pr"/>
      </pattern>
      <action>
        <let>
          <var n="prep-lema"/><clip pos="1" part="lem" side="sl"/>
        </let>
        <out>
          <chunk namefrom="prep-lema" case="caseFirstWord">
            <tags>
              <tag><lit-tag v="PR"/></tag>
            </tags>
            <lu>
              <clip pos="1" part="whole" side="tl"/>
            </lu>
          </chunk>
        </out>
      </action>
    </rule>

    <rule comment="lone det removals">
      <pattern>
        <pattern-item n="det"/>
      </pattern>
      <action>
        <call-macro n="artmangle1">
          <with-param pos="1"/>
        </call-macro>
        <out>
          <chunk name="unkdet" case="caseFirstWord">
            <tags>
              <tag><lit-tag v="DET"/></tag>
            </tags>
            <!-- meh -->
            <lu>
              <var n="determiner"/>
            </lu>
          </chunk>
        </out>
      </action>
    </rule>

    <rule comment="stray inf to infa, what cna you do">
      <pattern>
          <pattern-item n="infverb"/> <!-- do -->
      </pattern>
      <action>
        <out>
          <chunk name="verb" case="caseFirstWord">
              <tags>
              <tag><lit-tag v="V-INF"/></tag>
            </tags>
            <lu>
              <clip pos="1" side="tl" part="lem"/> <!-- tehdä -->
              <clip pos="1" side="tl" part="a_verb"/> <!-- vblex.actv -->
              <lit-tag v="actv"/>
              <lit-tag v="infa"/> <!-- infa -->
              <lit-tag v="sg"/> <!-- sg -->
              <lit-tag v="lat"/> <!-- lat -->
            </lu>
          </chunk>
        </out>
      </action>
    </rule>
    <rule comment="Säätö: stray negation">
      <pattern>
        <pattern-item n="notadv"/>
      </pattern>
      <action>
        <out>
          <chunk name="unkneg" case="caseFirstWord">
            <tags>
              <tag><lit-tag v="NEG"/></tag>
            </tags>
            <lu>
              <lit v="ei"/>
                <lit-tag v="vaux.neg.actv.p3.sg"/>
            </lu>
          </chunk>
        </out>
      </action>
    </rule>

    <rule comment="REGLA: SENT">
      <pattern>
        <pattern-item n="sent"/>
      </pattern>
      <action>
        <out>
          <chunk name="sent" case="caseFirstWord">
            <tags>
              <tag><lit-tag v="SENT"/></tag>
            </tags>
            <lu>
              <clip pos="1" side="tl" part="whole"/>
            </lu>
          </chunk>
        </out>
      </action>
    </rule>

  </section-rules>
</transfer>
